		"lucide-svelte": "latest",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"src": "latest",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-hamburgers": "latest",
		"tailwindcss": "^4.0.0",
		"typescript": "5.7.3",
		"vite": "^6.2.6"
	},
	"dependencies": {
		"@types/aos": "^3.0.7",
		"aos": "^2.3.4",
